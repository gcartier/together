(package launch
  
  (title "Launch")
  (description "Together Launch")
  (authors "Guillaume Cartier")
  (stage alpha)
  (root "src")
  (products (launch (title "Launch")
                    (prefix "Launch")
                    (product launch.product)
                    (run launch.run)
                    (build (launch executable: "Together" minimum-heap: 500000 bundle: "together" resources: "resources/together"))
                    (update launch.run
