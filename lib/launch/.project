(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: launch description-file: {File :context ".package"}
    (<Project-Directory>                directory: {Directory :context "src"}
      (<Project-Directory>              directory: {Directory :context "src" "launch"}
        (<Jazz-File>       name: run    source: {File :context "src" "launch" "run.jazz"}))))))
