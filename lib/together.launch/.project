;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>               name: launch  description-file: {File :context ".package"}
    (<Project-Directory>                 directory: {Directory :context "src"}
      (<Project-Directory>               directory: {Directory :context "src" "together"}
        (<Jazz-File>       name: launch  source: {File :context "src" "together" "launch.jazz"})
        (<Jazz-File>       name: product source: {File :context "src" "together" "product.jazz"})
        (<Jazz-File>       name: update  source: {File :context "src" "together" "update.jazz"}))))))
