        (<Jazz-File>       name: boot-processor     source: {File :context "src" "together" "boot-processor.jazz"})
        (<Jazz-File>       name: boot-server        source: {File :context "src" "together" "boot-server.jazz"})
        (<Jazz-File>       name: product-processor  source: {File :context "src" "together" "product-processor.jazz"})
