;;;; Project
;;;


(data jazz.ide.data.project


(import (jazz.project)
        (jazz.editor.jazz))


(form
  (<Project>                 name: together           description-file: {File :context ".package"}
    (<*>                                              tag-reference: {File :context "together-circle.project"})
    (<*>                                              tag-reference: {File :context "together-sphere.project"})
    (<*>                                              tag-reference: {File :context "together-worker.project"})
    (<Project-Directory>                              directory: {Directory :context "src"}
      (<Project-Directory>                            directory: {Directory :context "src" "together"}
        (<Jazz-File>         name: _together          source: {File :context "src" "together" "_together.jazz"})
        (<Jazz-File>         name: boot-together      source: {File :context "src" "together" "boot-circle.jazz"})
        (<Jazz-File>         name: boot               source: {File :context "src" "together" "boot-sphere.jazz"})
        (<Jazz-File>         name: boot-worker        source: {File :context "src" "together" "boot-worker.jazz"})
        (<Jazz-File>         name: debugger           source: {File :context "src" "together" "debugger.jazz"})
        (<Jazz-File>         name: devel              source: {File :context "src" "together" "devel.jazz"})
        (<Jazz-File>         name: product-together   source: {File :context "src" "together" "product-circle.jazz"})
        (<Jazz-File>         name: product            source: {File :context "src" "together" "product-sphere.jazz"})
        (<Jazz-File>         name: product-worker     source: {File :context "src" "together" "product-worker.jazz"})
        (<Jazz-File>         name: product-world      source: {File :context "src" "together" "product-world.jazz"})
        (<Jazz-File>         name: run-together       source: {File :context "src" "together" "run-circle.jazz"})
        (<Jazz-File>         name: run                source: {File :context "src" "together" "run-sphere.jazz"})
        (<Jazz-File>         name: run-worker         source: {File :context "src" "together" "run-worker.jazz"})
        (<Jazz-File>         name: script             source: {File :context "src" "together" "script.jazz"})
        (<Jazz-File>         name: settings           source: {File :context "src" "together" "settings.jazz"})
        (<Jazz-File>         name: window             source: {File :context "src" "together" "window.jazz"})
        (<Jazz-File>         name: windows            source: {File :context "src" "together" "windows.jazz"})
        (<Jazz-File>         name: worker-application source: {File :context "src" "together" "worker-application.jazz"})
        (<Jazz-File>         name: worker-process     source: {File :context "src" "together" "worker-process.jazz"})
        (<Jazz-File>         name: worker-settings    source: {File :context "src" "together" "worker-settings.jazz"})
        (<Jazz-File>         name: zone               source: {File :context "src" "together" "zone.jazz"})
        (<Project-Directory>                          directory: {Directory :context "src" "together" "circle"}
          (<Jazz-File>       name: appl               source: {File :context "src" "together" "circle" "appl.jazz"})
          (<Jazz-File>       name: tier               source: {File :context "src" "together" "circle" "tier.jazz"})
          (<Jazz-File>       name: view               source: {File :context "src" "together" "circle" "view.jazz"}))
        (<Project-Directory>                          directory: {Directory :context "src" "together" "sphere"}
          (<Jazz-File>       name: appl               source: {File :context "src" "together" "sphere" "appl.jazz"})))
      (<Jazz-File>           name: t                  source: {File :context "src" "t.jazz"})
      (<Jazz-File>           name: trun               source: {File :context "src" "trun.jazz"})))))
